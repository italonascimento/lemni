{
  "name": "@lemni/core",
  "version": "0.0.3",
  "description": "A reactive framework based on React",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "author": "Denis Ikeda, √çtalo Nascimento",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/italonascimento/lemni"
  },
  "readme": "README.md",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf lib && mkdir lib",
    "build": "npm run clean && tsc",
    "test": "jest --verbose",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "./tests/.*\\.test\\.(ts|tsx|js)$"
  },
  "devDependencies": {
    "@lemni/tsconfig": "^0.0.2",
    "@lemni/tslint": "^0.0.2",
    "@types/enzyme": "^3.1.8",
    "@types/jest": "^22.0.1",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.3.4",
    "@types/sinon": "^4.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.1.4",
    "react-dom": "^16.2.0",
    "sinon": "^4.2.2",
    "ts-jest": "^22.0.3",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.3.1",
    "xstream": "^11.2.0"
  }
}
