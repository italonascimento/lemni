{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "interface-name": ["never-prefix"],
    "member-access": false,
    "no-consecutive-blank-lines": [false],
    "no-console": [false],
    "no-empty": false,
    "no-empty-interface": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "trailing-comma": [false]
  },
  "rulesDirectory": []
}